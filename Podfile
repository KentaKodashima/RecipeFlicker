  pod 'Koloda', '~> 4.5'
  pod 'Kingfisher', '~> 4.0'

  target 'RecipeFlickerTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'RecipeFlickerUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
